.csdn-side-toolbar a.option-box[data-type="gotop"].go-top-hide { visibility: hidden; }

.csdn-side-toolbar a.option-box.active img, .csdn-side-toolbar a.option-box:hover img { display: none; }

.csdn-side-toolbar a.option-box.active .show-txt, .csdn-side-toolbar a.option-box:hover .show-txt { display: flex; opacity: 100; }

.csdn-side-toolbar a.option-box.no—h.active img, .csdn-side-toolbar a.option-box.no—h:hover img { display: block; }

.csdn-side-toolbar a.option-box.no—h.active .show-txt, .csdn-side-toolbar a.option-box.no—h:hover .show-txt { display: none; }

.csdn-side-toolbar a.option-box img { display: block; width: 24px; }

.csdn-side-toolbar a.option-box .show-txt { display: none; position: absolute; inset: 0px; border-radius: 50%; -webkit-box-align: center; align-items: center; -webkit-box-pack: center; justify-content: center; font-size: 12px; line-height: 14px; color: rgb(153, 154, 170); background-color: rgb(255, 255, 255); text-align: center; opacity: 0; transition: opacity 0.3s ease-in-out 0s; }

.csdn-side-toolbar.black a.option-box { background: rgb(68, 68, 71); }

.csdn-side-toolbar.black a.option-box .show-txt { background: rgb(68, 68, 71); }

.csdn-side-toolbar.black a.option-box .app-qr-box .bg-box { background: rgb(68, 68, 71); box-shadow: rgba(0, 0, 0, 0.1) 0px 2px 8px 0px; border-radius: 4px; }

.csdn-side-toolbar.black a.option-box .app-qr-box .tab-pane .tab-item { color: rgb(153, 153, 153); border-color: rgba(232, 232, 237, 0.4); }

.csdn-side-toolbar.black a.option-box .app-qr-box .tab-pane .tab-item:hover { background: rgba(204, 204, 216, 0.3); }

.csdn-side-toolbar.black a.option-box .app-qr-box .tab-pane .tab-content { border-color: rgba(232, 232, 237, 0.4); background: rgb(68, 68, 71); }

.csdn-side-toolbar.black a.option-box .app-qr-box .tab-pane .tab-content p.desc { color: rgb(153, 153, 153); }

